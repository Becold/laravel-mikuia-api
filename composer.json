{
    "name": "becold/laravel-mikuia-api",
    "description": "RESTful Mikuia API for Laravel 5",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Romain Marbaix",
            "email": "contact@romainmarbaix.be"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse --level=5 src tests"
    },
    "autoload": {
        "psr-4": {
            "Becold\\MikuiaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Becold\\MikuiaApi\\Tests\\": "tests"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.3",
        "phpstan/phpstan": "^0.12.36"
    }
}
